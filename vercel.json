{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/mock" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Vercel-Mock-Mode",
          "value": "true"
        }
      ]
    }
  ]
}