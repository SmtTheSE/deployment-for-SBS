{
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/mock" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Vercel-Mock-Mode",
          "value": "true"
        }
      ]
    }
  ],
  "env": {
    "VITE_USE_MOCK": "true"
  }
}